{
  "name": "eugmerkeleon/laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Kozlov",
      "email": "ekozlov@merkeleon.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": ">=5.7",
    "minime/annotations": "2.3.1"
  },
  "autoload": {
    "psr-4": {
      "EugMerkeleon\\Support\\AutoDoc\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "EugMerkeleon\\Support\\AutoDoc\\AutoDocServiceProvider"
      ]
    }
  }
}
